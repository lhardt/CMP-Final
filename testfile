com divisao = 5 / 3;
com mod = 5 % 3;
com soma = 5 + 3;
com subtr = 5 - 3;
com menor = 5 < 3;
com menor = 5 > 3;
com indice = vars[5];
com parent = vars + (5);
com virgula = vars , vars;
com ampersand = vars & vars;
com pipe = vars | vars;


com leq := 5 <= 3;
com geq := 5 >= 3;
com  eq := 5 == 3;
com set := 5 -> 3;


