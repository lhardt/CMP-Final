// Um primeiro comentário! :) 
var minhaAltura tipo decimal := 1.75;
var minhaIdade tipo inteiro := 25;

enquanto(minhaAltura > 10 * minhaIdade){
  minhaAltura -= 1;
  minhaIdade += 1;
}

se( ! ( meuNome != seuNome ) ){ 
  retorna 0; // pessoas diferentes!
} senao {
  retorna 1; // pessoas iguais!
}

